        <servlet-name>hapuspengguna</servlet-name>
        <url-pattern>/admin/hapuspengguna</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>konfirm</servlet-name>
        <url-pattern>/admin/konfirm</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
</web-app>
