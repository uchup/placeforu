    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>servlet.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controller</servlet-name>
        <servlet-class>controller.ServletController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>servlet.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>servlet.HomeAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pemilik</servlet-name>
        <servlet-class>servlet.HomePemilik</servlet-class>
