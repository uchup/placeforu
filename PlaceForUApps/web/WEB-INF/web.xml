    <servlet>
        <servlet-name>penyewa</servlet-name>
        <servlet-class>servlet.HomePenyewa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profilPemilik</servlet-name>
        <servlet-class>servlet.ProfilPemilik</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profilpenyewa</servlet-name>
        <servlet-class>servlet.ProfilPenyewa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editpemilik</servlet-name>
